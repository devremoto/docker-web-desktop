{
  "name": "docker-web-desktop-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "start": "vite --open --mode development",
    "dev": "vite --open  --mode development",
    "dev:docker": "vite --mode docker --open",
    "build": "vue-tsc && vite build",
    "build:production": "vue-tsc && vite build --mode production",
    "build:docker": "vue-tsc && vite build --mode docker",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "preview:docker": "vite preview --mode docker",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "pinia": "^3.0.4",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.5.1",
    "typescript": "^5.4.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^2.0.0"
  }
}